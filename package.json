{
  "name": "gatsby-serif-theme",
  "version": "1.0.0",
  "author": "Robert Austin <rob@jugglerdigital.com>",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "gatsby build && npm run add-domain && gh-pages -d public -b master",
    "lint": "tsc && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "graphql-types": "graphql-codegen init",
    "gen-types": "graphql-codegen --config codegen.yml",
    "generate": "graphql-codegen --config codegen.yml",
    "add-domain": "echo \"optimax.dev\" > public/CNAME"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@types/react-helmet": "^5.0.8",
    "classnames": "^2.2.6",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^1.1.1",
    "gatsby": "^2.9.7",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-svg-sprite": "^2.0.1",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-source-filesystem": "^2.0.41",
    "gatsby-transformer-json": "^2.1.6",
    "gatsby-transformer-remark": "^2.4.0",
    "graphql-code-generator": "^0.18.2",
    "node-sass": "^4.12.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.1",
    "react-responsive": "^7.0.0",
    "react-scroll": "^1.7.12",
    "react-share": "^3.0.0",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.3.1",
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-scroll": "^1.5.4",
    "@types/react-share": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "gatsby-plugin-eslint": "^2.0.5",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "prettier-eslint": "^8.8.2"
  }
}
